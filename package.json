{
  "private": true,
  "repository": "github:cibernox/ember-power-calendar-moment",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "pnpm run --filter ember-power-calendar-moment build",
    "lint": "pnpm run lint --filter --if-present",
    "lint:fix": "pnpm run lint:fix --filter --if-present",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:addon": "npm start --filter ember-power-calendar-moment -- --no-watch.clearScreen",
    "start:test-app": "npm start --filter test-app"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "prettier": "^2.5.1"
  },
  "workspaces": [
    "ember-power-calendar-moment",
    "test-app"
  ],
  "engines": {
    "node": "16.* || >= 18"
  }
}